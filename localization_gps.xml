<launch>
<node pkg="robot_localization" type="navsat_transform_node" name="navsat_node">
	<param name="frequency" value="30"/>
	<param name="sensor_timeout" value="0.1"/>  
	<param name="two_d_mode" value="true"/>

	<param name="map_frame" value="map"/>
	<param name="odom_frame" value="odom"/>
	<param name="base_link_frame" value="base_link"/>
	<param name="world_frame" value="map"/>

    <param name="zero_altitude" value="true"/>

    <!-- If this is true, the transform world_frame->utm transform is broadcast 
    for use by other nodes. -->
    <param name="broadcast_utm_transform" value="true"/>

    <param name="magnetic_declination_radians" value="0.216666667"/>

    <!-- input topics -->
    <remap from="imu/data" to="imu_mag_fused" />
    <remap from="gps/fix" to="gps_data" />
    <remap from="odometry/filtered" to="odometry_odom" />

    <!-- output topic -->
    <remap from="odometry/gps" to="odometry_gps" /> 

</node>
</launch>
